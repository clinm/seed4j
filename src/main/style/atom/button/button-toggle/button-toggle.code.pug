include button-toggle.mixin.pug

+seed4j-button-toggle Toggle
+seed4j-button-toggle({ active: true }) Toggle active
+seed4j-button-toggle({ disabled: true }) Toggle disabled
+seed4j-button-toggle({ shortName: 'JS', fullName: 'JavaScript' })
+seed4j-button-toggle({ shortName: 'TS', fullName: 'TypeScript', active: true })
+seed4j-button-toggle({ shortName: 'CSS', fullName: 'Cascading Style Sheets', disabled: true })
